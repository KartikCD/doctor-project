  <body>
    <% if (type === "Doctor") { %>

    <h2>Your Profile</h2>
    <table class="table-2">
      <tr>
        <th>Field</th>
        <th>Information</th>
      </tr>
      <tr>
        <td>Name</td>
        <td><%- info.Name %></td>
      </tr>
      <tr>
        <td>Phone Number</td>
        <td><%- info.PhoneNumber %></td>
      </tr>
      <tr>
        <td>Email</td>
        <td><%- info.email %></td>
      </tr>
      <tr>
        <td>DOB</td>
        <td><%- info.date_of_birth %></td>
      </tr>
      <tr>
        <td>doctor_license_number</td>
        <td><%- info.doctor_license_number %></td>
      </tr>
    </table>
