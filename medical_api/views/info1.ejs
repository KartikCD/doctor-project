      <tr>
        <th>Name</th>
        <th>Phone Number</th>
        <th>Email</th>
        <th>DOB</th>
        <th>Doctor License Number</th>
      </tr>
      <% for( let index = 0; index < info.assistantDoctor.length; index++ ) { %>

      <tr>
        <td><%- info.assistantDoctor[index].Name %></td>
        <td><%- info.assistantDoctor[index].PhoneNumber %></td>
        <td><%- info.assistantDoctor[index].email %></td>
        <td><%- info.assistantDoctor[index].date_of_birth %></td>
